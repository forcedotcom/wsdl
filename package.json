{
  "name": "@salesforce/types",
  "version": "1.5.0",
  "description": "contains typescript types for salesforce objects",
  "repository": "forcedotcom/wsdl",
  "exports": {
    "./apex": "./lib/apex.js",
    "./metadata": "./lib/metadata.js",
    "./partner": "./lib/partner.js",
    "./tooling": "./lib/tooling.js"
  },
  "types": "./src",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "generate": "ts-node scripts/generate.ts",
    "lint": "wireit",
    "prepare": "husky install",
    "prepack": "sf-prepack",
    "build": "wireit",
    "test": "mocha"
  },
  "wireit": {
    "build": {
      "command": "tsc -p . --pretty --pretty",
      "clean": "if-file-deleted",
      "files": [
        "src/**/*.ts",
        "tsconfig.json"
      ],
      "output": [
        "lib/**",
        ".tsbuildinfo"
      ]
    },
    "lint": {
      "command": "eslint src --color --cache --cache-location .eslintcache --fix",
      "files": [
        "src/**/*.ts",
        "tsconfig.json"
      ],
      "output": []
    }
  },
  "keywords": [
    "salesforce",
    "sf",
    "metadata"
  ],
  "author": "Salesforce",
  "contributors": [
    {
      "name": "Andrés Catalán",
      "url": "https://github.com/catalandres"
    }
  ],
  "license": "BSD-3-Clause",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/jsforce/jsforce/raw/master/LICENSE"
    }
  ],
  "files": [
    "/lib"
  ],
  "devDependencies": {
    "@salesforce/dev-scripts": "^9.0.0",
    "@types/xml2js": "^0.4.14",
    "eslint-plugin-sf-plugin": "^1.18.3",
    "husky": "^8.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "wireit": "^0.14.12",
    "xml2js": "^0.6.2"
  }
}
